{
  "name": "evisa-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:prod": "npm run prod",
    "start": "nodemon",
    "serve": "node ./dist/bin/www",
    "transpile": "babel ./app --out-dir dist",
    "clean": "rimraf dist",
    "build": "npm-run-all clean transpile",
    "dev": "npm-run-all build serve",
    "prod": "npm-run-all build serve",
    "watch:dev": "nodemon",
    "test": "jest"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3,
          "targets": "> 0.25%, not dead"
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "app/*",
      "public/*",
      "views/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "body-parser": "^1.20.0",
    "connect-sqlite3": "^0.9.11",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.22.8",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.7",
    "express": "^4.17.0",
    "express-openid-connect": "^2.7.2",
    "express-session": "^1.17.2",
    "form-data": "^4.0.0",
    "http-errors": "~1.6.3",
    "mailgun.js": "^7.0.2",
    "mkdirp": "^1.0.4",
    "morgan": "~1.9.1",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.5",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "passport": "^0.5.2",
    "passport-google-oidc": "^0.1.0",
    "pluralize": "^8.0.0",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.6",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "jest": "^27.5.1"
  }
}
